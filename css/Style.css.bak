.bg-video-wrap {
  width: 95%;
  margin: auto;
  position: relative;
  top: 60px;
  z-index: 2;
}
.container-fluid.v-parent {
  background: url('../images/veasy-1.gif');
  background-size: 100% 400px;
  background-repeat: no-repeat;
}
.bg-video-wrap .vedio-play {
  border-radius: 9px;
  height: 100%;
  width: 100%;
  filter: brightness(0.5);
}
.bg-video-wrap .play-icon i {
  position: absolute;
  z-index: 1;
  top: -9%;
  right: -9%;
  font-size: 60px;
  color: #000d51 !important;
}
.bg-video-wrap .play-icon {
  position: absolute;
  left: 50%;
  z-index: 1;
  top: 50%;
  cursor: pointer;
  transform: translateX(-50%) translateY(-50%);
  width: 50px;
  height: 50px;
}
.bg-video-wrap .play-icon:before {
  content: "";
  position: relative;
  display: block;
  width: 300%;
  z-index: 1;
  height: 300%;
  box-sizing: border-box;
  margin-left: -100%;
  margin-top: -100%;
  border-radius: 50%;
  background-color: white;
  animation: pulse-ring 4s cubic-bezier(0.215, 0.61, 0.355, 1) infinite;
  -webkit-animation: pulse-ring 4s cubic-bezier(0.215, 0.61, 0.355, 1) infinite;
}
@keyframes pulse-ring {
  0% {
    transform: scale(0.63);
  }
  80%,
  100% {
    opacity: 1;
  }
}
@keyframes pulse-dot {
  0% {
    transform: scale(0.8);
  }
  50% {
    transform: scale(1);
  }
  100% {
    transform: scale(0.8);
  }
}


